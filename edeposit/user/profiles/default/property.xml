<?xml version="1.0"?>
<site  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
       i18n:domain="edeposit.user"
>
  <property name="title" i18n:translate="">E-Deposit Site</property>
  <property name="description" i18n:translate="">Welcome to E-Deposit</property>
  <property name="selectable_views" type="lines">
    <element value="folder_listing"/>
    <element value="news_listing"/>
  </property>
  <property name="email_from_address" type="string">edeposit@nkp.cz</property>
  <property name="email_from_name" type="string">E-Deposit</property>
  <property name="validate_email" type="boolean">True</property>
  <property name="email_charset" type="string">utf-8</property>
  <property name="enable_permalink" type="boolean">False</property>
  <property name="default_page" type="string">front-page</property>
</site>
